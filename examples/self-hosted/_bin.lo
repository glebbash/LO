include "../lib/args.lo"
include "../lib/print.lo"
include "../lib/std.lo"

export memory {
    min_pages: 1,
}

const USAGE = "Usage:
  lo lex <input.lo> (debug)"

export fn _start() {
    let args = Args::load()
    defer args.free()

    if args.argc < 3 {
        puts(USAGE)
        puts("\n")
        return
    }

    puts("WIP\n")
}
