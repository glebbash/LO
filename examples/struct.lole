(external-fn printf ((& i8) i32) i32)

(struct List
  (size i32)
  (items (& ([] i32)))
)

(fn print_list ((list (& List)) (index i32)) i32
  (printf "%d\n" (get (get list 0 List/items) 0 index))

  (if (< (+ index 1) (get list 0 List/size))
    (print_list list (+ index 1))
    0
  )
)

(fn main () i32
  (def x List)
  (set x 0 List/size 3)
  (set x 0 List/items (array 1 2 3))

  (print_list x 0)
)
