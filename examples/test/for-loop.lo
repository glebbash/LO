export fn main(): u32 {
    let result = 0

    for i in 0..10 {
        result += i
    }

    for i in 0..10 {
        result += i
        if i == 7 {
            break
        }
    }

    for i in 0..10 {
        if i == 4 {
            continue
        }
        result += i
    }

    for i in 0..10 {
        if i == 4 {
            continue
        }
        result += i
        if i == 7 {
            break
        }
    }

    return result
}
