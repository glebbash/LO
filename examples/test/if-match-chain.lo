include "../lib/cli.lo"

enum Dir {
    Left(u32),
    Right(u32),
}

fn main() {
    go(Dir::Left(5))
    go(Dir::Right(3))
}

fn go(dir: Dir) {
    if match Dir::Left(steps) = dir {
        puts("going left ")
        puts(steps.to_tmp_str!())
        puts(" steps\n")
    } else if match Dir::Right(steps) = dir {
        puts("going right ")
        puts(steps.to_tmp_str!())
        puts(" steps\n")
    }
}
