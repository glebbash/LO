memory { min_pages: 1 }
let MEM_CURRENT = 0

fn main() {
    let b = Box::new!(1)
    let v = b.get!()
}

type Box = struct {
    raw: &void,
}

inline fn Box::new!(value: infer(T)): Box(T) {
    let self = Box { raw: MEM_CURRENT as &void }
    MEM_CURRENT += sizeof T
    self
}

inline fn Box::get!(self): itemof(typeof(self)) {
    *(self.raw as &itemof(typeof(self)))
}
