export fn main() -> u32 {
    let p1 = Point2D { x: 0, y: 0 };
    p1.x = 1;
    p1.y = 2;

    let sum = p1.add(Point2D { x: 1, y: 1 });
    return sum.len_squared();
};

struct Point2D {
    x: u32,
    y: u32,
};

fn Point2D::add(self, other: Point2D) -> Point2D {
    return Point2D {
        x: self.x + other.x,
        y: self.y + other.y,
    };
};

fn Point2D::len_squared(self) -> u32 {
    return self.x * self.x + self.y * self.y;
};
