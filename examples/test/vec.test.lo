// exports vec api for testing

@include("../lib/std.lo")

@use_memory(min_pages = 1, exported)

@export_existing(Vec::with_capacity, out = "vec_new")
@export_existing(Vec::swap, out = "vec_swap")
@export_existing(Vec::push_all, out = "vec_push_all")

export fn vec_len(vec: &Vec): u32 {
    return vec.size
}

export fn vec_push_u8(vec: &Vec(u8), item: u8) {
    vec.push!(item)
}

export fn vec_get_u8(vec: &Vec(u8), index: u32): u8 {
    return vec.get!(index)
}
