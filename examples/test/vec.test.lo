// exports vec api for testing
export memory {
    min_pages: 20,
}

include "../lib/std.lo"

try export Vec::with_capacity as "vec_new"

try export Vec::swap as "vec_swap"

try export Vec::push_all as "vec_push_all"

export fn vec_len(vec: &Vec): u32 {
    return vec.size
}

export fn vec_push_u8(vec: &Vec, item: u8) {
    vec.push!(item)
}

export fn vec_get_u8(vec: &Vec, index: u32): u8 {
    return vec.get!<u8>(index)
}
