{
  "name": "lo",
  "displayName": "LO",
  "description": "LO language support",
  "version": "0.0.70",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": "https://github.com/glebbash/lo",
  "contributes": {
    "languages": [
      {
        "id": "lo",
        "aliases": [
          "lo"
        ],
        "extensions": [
          ".lo"
        ],
        "configuration": "./syntaxes/lo.config.json",
        "icon": {
          "light": "./icons/lo.svg",
          "dark": "./icons/lo.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "lo",
        "scopeName": "source.lo",
        "path": "./syntaxes/lo.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^2.20.1"
  },
  "scripts": {
    "package": "npm version patch && npx @vscode/vsce package"
  }
}
