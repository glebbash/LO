{
  "name": "lo",
  "displayName": "LO",
  "description": "LO language support",
  "version": "0.0.70",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": "https://github.com/glebbash/lo",
  "contributes": {
    "languages": [
      {
        "id": "lo-lisp",
        "aliases": [
          "lo-lisp"
        ],
        "extensions": [
          ".lole"
        ],
        "configuration": "./syntaxes/lo-lisp.config.json",
        "icon": {
          "light": "./icons/lo-lisp.svg",
          "dark": "./icons/lo-lisp.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "lo-lisp",
        "scopeName": "source.lole",
        "path": "./syntaxes/lo-lisp.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^2.20.1"
  },
  "scripts": {
    "package": "npm version patch && npx @vscode/vsce package"
  }
}
