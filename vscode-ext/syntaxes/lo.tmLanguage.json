{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "comment": "adapted from https://github.com/dustypomerleau/rust-syntax/blob/main/syntaxes/rust.tmLanguage.json",
    "name": "lo",
    "fileTypes": ["lo"],
    "scopeName": "source.lo",
    "patterns": [
        {
            "comment": "boxed slice literal",
            "begin": "(<)(\\[)",
            "beginCaptures": {
                "1": {
                    "name": "punctuation.brackets.angle.lo"
                },
                "2": {
                    "name": "punctuation.brackets.square.lo"
                }
            },
            "end": ">",
            "endCaptures": {
                "0": {
                    "name": "punctuation.brackets.angle.lo"
                }
            },
            "patterns": [
                {
                    "include": "#block-comments"
                },
                {
                    "include": "#comments"
                },
                {
                    "include": "#gtypes"
                },
                {
                    "include": "#lvariables"
                },
                {
                    "include": "#lifetimes"
                },
                {
                    "include": "#punctuation"
                },
                {
                    "include": "#types"
                }
            ]
        },
        {
            "comment": "macro type metavariables",
            "name": "meta.macro.metavariable.type.lo",
            "match": "(\\$)((crate)|([A-Z][A-Za-z0-9_]*))((:)(block|expr|ident|item|lifetime|literal|meta|path?|stmt|tt|ty|vis))?",
            "captures": {
                "1": {
                    "name": "keyword.operator.macro.dollar.lo"
                },
                "3": {
                    "name": "keyword.other.crate.lo"
                },
                "4": {
                    "name": "entity.name.type.metavariable.lo"
                },
                "6": {
                    "name": "keyword.operator.key-value.lo"
                },
                "7": {
                    "name": "variable.other.metavariable.specifier.lo"
                }
            },
            "patterns": [
                {
                    "include": "#keywords"
                }
            ]
        },
        {
            "comment": "macro metavariables",
            "name": "meta.macro.metavariable.lo",
            "match": "(\\$)([a-z][A-Za-z0-9_]*)((:)(block|expr|ident|item|lifetime|literal|meta|path?|stmt|tt|ty|vis))?",
            "captures": {
                "1": {
                    "name": "keyword.operator.macro.dollar.lo"
                },
                "2": {
                    "name": "variable.other.metavariable.name.lo"
                },
                "4": {
                    "name": "keyword.operator.key-value.lo"
                },
                "5": {
                    "name": "variable.other.metavariable.specifier.lo"
                }
            },
            "patterns": [
                {
                    "include": "#keywords"
                }
            ]
        },
        {
            "comment": "macro rules",
            "name": "meta.macro.rules.lo",
            "match": "\\b(macro_rules!)\\s+(([a-z0-9_]+)|([A-Z][a-z0-9_]*))\\s+(\\{)",
            "captures": {
                "1": {
                    "name": "entity.name.function.macro.rules.lo"
                },
                "3": {
                    "name": "entity.name.function.macro.lo"
                },
                "4": {
                    "name": "entity.name.type.macro.lo"
                },
                "5": {
                    "name": "punctuation.brackets.curly.lo"
                }
            }
        },
        {
            "comment": "modules",
            "match": "(mod)\\s+((?:r#(?!crate|[Ss]elf|super))?[a-z][A-Za-z0-9_]*)",
            "captures": {
                "1": {
                    "name": "storage.type.lo"
                },
                "2": {
                    "name": "entity.name.module.lo"
                }
            }
        },
        {
            "comment": "external crate imports",
            "name": "meta.import.lo",
            "begin": "\\b(extern)\\s+(crate)",
            "beginCaptures": {
                "1": {
                    "name": "storage.type.lo"
                },
                "2": {
                    "name": "keyword.other.crate.lo"
                }
            },
            "end": ";",
            "endCaptures": {
                "0": {
                    "name": "punctuation.semi.lo"
                }
            },
            "patterns": [
                {
                    "include": "#block-comments"
                },
                {
                    "include": "#comments"
                },
                {
                    "include": "#keywords"
                },
                {
                    "include": "#punctuation"
                }
            ]
        },
        {
            "comment": "use statements",
            "name": "meta.use.lo",
            "begin": "\\b(use)\\s",
            "beginCaptures": {
                "1": {
                    "name": "keyword.other.lo"
                }
            },
            "end": ";",
            "endCaptures": {
                "0": {
                    "name": "punctuation.semi.lo"
                }
            },
            "patterns": [
                {
                    "include": "#block-comments"
                },
                {
                    "include": "#comments"
                },
                {
                    "include": "#keywords"
                },
                {
                    "include": "#namespaces"
                },
                {
                    "include": "#punctuation"
                },
                {
                    "include": "#types"
                },
                {
                    "include": "#lvariables"
                }
            ]
        },
        {
            "include": "#block-comments"
        },
        {
            "include": "#comments"
        },
        {
            "include": "#attributes"
        },
        {
            "include": "#lvariables"
        },
        {
            "include": "#constants"
        },
        {
            "include": "#gtypes"
        },
        {
            "include": "#functions"
        },
        {
            "include": "#types"
        },
        {
            "include": "#keywords"
        },
        {
            "include": "#lifetimes"
        },
        {
            "include": "#macros"
        },
        {
            "include": "#namespaces"
        },
        {
            "include": "#punctuation"
        },
        {
            "include": "#strings"
        },
        {
            "include": "#variables"
        }
    ],
    "repository": {
        "comments": {
            "patterns": [
                {
                    "comment": "documentation comments",
                    "name": "comment.line.documentation.lo",
                    "match": "^\\s*///.*"
                },
                {
                    "comment": "line comments",
                    "name": "comment.line.double-slash.lo",
                    "match": "\\s*//.*"
                }
            ]
        },
        "block-comments": {
            "patterns": [
                {
                    "comment": "empty block comments",
                    "name": "comment.block.lo",
                    "match": "/\\*\\*/"
                },
                {
                    "comment": "block documentation comments",
                    "name": "comment.block.documentation.lo",
                    "begin": "/\\*\\*",
                    "end": "\\*/",
                    "patterns": [
                        {
                            "include": "#block-comments"
                        }
                    ]
                },
                {
                    "comment": "block comments",
                    "name": "comment.block.lo",
                    "begin": "/\\*(?!\\*)",
                    "end": "\\*/",
                    "patterns": [
                        {
                            "include": "#block-comments"
                        }
                    ]
                }
            ]
        },
        "constants": {
            "patterns": [
                {
                    "comment": "ALL CAPS constants",
                    "name": "constant.other.caps.lo",
                    "match": "\\b[A-Z]{2}[A-Z0-9_]*\\b"
                },
                {
                    "comment": "constant declarations",
                    "match": "\\b(const)\\s+([A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "storage.type.lo"
                        },
                        "2": {
                            "name": "constant.other.caps.lo"
                        }
                    }
                },
                {
                    "comment": "decimal integers and floats",
                    "name": "constant.numeric.decimal.lo",
                    "match": "\\b\\d[\\d_]*(\\.?)[\\d_]*(?:(E|e)([+-]?)([\\d_]+))?(f32|f64|i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
                    "captures": {
                        "1": {
                            "name": "punctuation.separator.dot.decimal.lo"
                        },
                        "2": {
                            "name": "keyword.operator.exponent.lo"
                        },
                        "3": {
                            "name": "keyword.operator.exponent.sign.lo"
                        },
                        "4": {
                            "name": "constant.numeric.decimal.exponent.mantissa.lo"
                        },
                        "5": {
                            "name": "entity.name.type.numeric.lo"
                        }
                    }
                },
                {
                    "comment": "hexadecimal integers",
                    "name": "constant.numeric.hex.lo",
                    "match": "\\b0x[\\da-fA-F_]+(i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.numeric.lo"
                        }
                    }
                },
                {
                    "comment": "octal integers",
                    "name": "constant.numeric.oct.lo",
                    "match": "\\b0o[0-7_]+(i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.numeric.lo"
                        }
                    }
                },
                {
                    "comment": "binary integers",
                    "name": "constant.numeric.bin.lo",
                    "match": "\\b0b[01_]+(i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.numeric.lo"
                        }
                    }
                },
                {
                    "comment": "booleans",
                    "name": "constant.language.bool.lo",
                    "match": "\\b(true|false)\\b"
                }
            ]
        },
        "escapes": {
            "comment": "escapes: ASCII, byte, Unicode, quote, regex",
            "name": "constant.character.escape.lo",
            "match": "(\\\\)(?:(?:(x[0-7][\\da-fA-F])|(u(\\{)[\\da-fA-F]{4,6}(\\}))|.))",
            "captures": {
                "1": {
                    "name": "constant.character.escape.backslash.lo"
                },
                "2": {
                    "name": "constant.character.escape.bit.lo"
                },
                "3": {
                    "name": "constant.character.escape.unicode.lo"
                },
                "4": {
                    "name": "constant.character.escape.unicode.punctuation.lo"
                },
                "5": {
                    "name": "constant.character.escape.unicode.punctuation.lo"
                }
            }
        },
        "attributes": {
            "comment": "attributes",
            "name": "meta.attribute.lo",
            "begin": "(#)(\\!?)(\\[)",
            "beginCaptures": {
                "1": {
                    "name": "punctuation.definition.attribute.lo"
                },
                "3": {
                    "name": "punctuation.brackets.attribute.lo"
                }
            },
            "end": "\\]",
            "endCaptures": {
                "0": {
                    "name": "punctuation.brackets.attribute.lo"
                }
            },
            "patterns": [
                {
                    "include": "#block-comments"
                },
                {
                    "include": "#comments"
                },
                {
                    "include": "#keywords"
                },
                {
                    "include": "#lifetimes"
                },
                {
                    "include": "#punctuation"
                },
                {
                    "include": "#strings"
                },
                {
                    "include": "#gtypes"
                },
                {
                    "include": "#types"
                }
            ]
        },
        "functions": {
            "patterns": [
                {
                    "comment": "exporting a function",
                    "match": "\\b(export)(\\()",
                    "captures": {
                        "1": {
                            "name": "keyword.other.lo"
                        },
                        "2": {
                            "name": "punctuation.brackets.round.lo"
                        }
                    }
                },
                {
                    "comment": "function definition",
                    "name": "meta.function.definition.lo",
                    "begin": "\\b(fn)\\s+((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)((\\()|(<))",
                    "beginCaptures": {
                        "1": {
                            "name": "keyword.other.fn.lo"
                        },
                        "2": {
                            "name": "entity.name.function.lo"
                        },
                        "4": {
                            "name": "punctuation.brackets.round.lo"
                        },
                        "5": {
                            "name": "punctuation.brackets.angle.lo"
                        }
                    },
                    "end": "\\{|;",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.curly.lo"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#block-comments"
                        },
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#constants"
                        },
                        {
                            "include": "#gtypes"
                        },
                        {
                            "include": "#functions"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#macros"
                        },
                        {
                            "include": "#namespaces"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#strings"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                },
                {
                    "comment": "function/method calls, chaining",
                    "name": "meta.function.call.lo",
                    "begin": "((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)(\\()",
                    "beginCaptures": {
                        "1": {
                            "name": "entity.name.function.lo"
                        },
                        "2": {
                            "name": "punctuation.brackets.round.lo"
                        }
                    },
                    "end": "\\)",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.round.lo"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#block-comments"
                        },
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#attributes"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#constants"
                        },
                        {
                            "include": "#gtypes"
                        },
                        {
                            "include": "#functions"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#macros"
                        },
                        {
                            "include": "#namespaces"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#strings"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                },
                {
                    "comment": "function/method calls with turbofish",
                    "name": "meta.function.call.lo",
                    "begin": "((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)(?=::<.*>\\()",
                    "beginCaptures": {
                        "1": {
                            "name": "entity.name.function.lo"
                        }
                    },
                    "end": "\\)",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.round.lo"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#block-comments"
                        },
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#attributes"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#constants"
                        },
                        {
                            "include": "#gtypes"
                        },
                        {
                            "include": "#functions"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#macros"
                        },
                        {
                            "include": "#namespaces"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#strings"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                }
            ]
        },
        "keywords": {
            "patterns": [
                {
                    "comment": "control flow keywords",
                    "name": "keyword.control.lo",
                    "match": "\\b(await|break|continue|do|else|for|if|loop|match|return|try|while|yield)\\b"
                },
                {
                    "comment": "storage keywords",
                    "name": "keyword.other.lo storage.type.lo",
                    "match": "\\b(extern|let|macro|mod)\\b"
                },
                {
                    "comment": "const keyword",
                    "name": "storage.modifier.lo",
                    "match": "\\b(const)\\b"
                },
                {
                    "comment": "type keyword",
                    "name": "keyword.declaration.type.lo storage.type.lo",
                    "match": "\\b(type)\\b"
                },
                {
                    "comment": "enum keyword",
                    "name": "keyword.declaration.enum.lo storage.type.lo",
                    "match": "\\b(enum)\\b"
                },
                {
                    "comment": "trait keyword",
                    "name": "keyword.declaration.trait.lo storage.type.lo",
                    "match": "\\b(trait)\\b"
                },
                {
                    "comment": "struct keyword",
                    "name": "keyword.declaration.struct.lo storage.type.lo",
                    "match": "\\b(struct)\\b"
                },
                {
                    "comment": "storage modifiers",
                    "name": "storage.modifier.lo",
                    "match": "\\b(abstract|static)\\b"
                },
                {
                    "comment": "other keywords",
                    "name": "keyword.other.lo",
                    "match": "\\b(as|async|become|box|dyn|move|final|impl|in|override|priv|pub|ref|typeof|union|unsafe|unsized|use|virtual|where|export|import|from|memory|defer|defer_eval|sizeof|include)\\b"
                },
                {
                    "comment": "fn",
                    "name": "keyword.other.fn.lo",
                    "match": "\\bfn\\b"
                },
                {
                    "comment": "crate",
                    "name": "keyword.other.crate.lo",
                    "match": "\\bcrate\\b"
                },
                {
                    "comment": "mut",
                    "name": "storage.modifier.mut.lo",
                    "match": "\\bmut\\b"
                },
                {
                    "comment": "logical operators",
                    "name": "keyword.operator.logical.lo",
                    "match": "(\\^|\\||\\|\\||&&|<<|>>|!)(?!=)"
                },
                {
                    "comment": "logical AND, borrow references",
                    "name": "keyword.operator.borrow.and.lo",
                    "match": "&(?![&=])"
                },
                {
                    "comment": "assignment operators",
                    "name": "keyword.operator.assignment.lo",
                    "match": "(\\+=|-=|\\*=|/=|%=|\\^=|&=|\\|=|<<=|>>=)"
                },
                {
                    "comment": "single equal",
                    "name": "keyword.operator.assignment.equal.lo",
                    "match": "(?<![<>])=(?!=|>)"
                },
                {
                    "comment": "comparison operators",
                    "name": "keyword.operator.comparison.lo",
                    "match": "(=(=)?(?!>)|!=|<=|(?<!=)>=)"
                },
                {
                    "comment": "math operators",
                    "name": "keyword.operator.math.lo",
                    "match": "(([+%]|(\\*(?!\\w)))(?!=))|(-(?!>))|(/(?!/))"
                },
                {
                    "comment": "less than, greater than (special case)",
                    "match": "(?:\\b|(?:(\\))|(\\])|(\\})))[ \\t]+([<>])[ \\t]+(?:\\b|(?:(\\()|(\\[)|(\\{)))",
                    "captures": {
                        "1": {
                            "name": "punctuation.brackets.round.lo"
                        },
                        "2": {
                            "name": "punctuation.brackets.square.lo"
                        },
                        "3": {
                            "name": "punctuation.brackets.curly.lo"
                        },
                        "4": {
                            "name": "keyword.operator.comparison.lo"
                        },
                        "5": {
                            "name": "punctuation.brackets.round.lo"
                        },
                        "6": {
                            "name": "punctuation.brackets.square.lo"
                        },
                        "7": {
                            "name": "punctuation.brackets.curly.lo"
                        }
                    }
                },
                {
                    "comment": "namespace operator",
                    "name": "keyword.operator.namespace.lo",
                    "match": "::"
                },
                {
                    "comment": "dereference asterisk",
                    "match": "(\\*)(?=\\w+)",
                    "captures": {
                        "1": {
                            "name": "keyword.operator.dereference.lo"
                        }
                    }
                },
                {
                    "comment": "subpattern binding",
                    "name": "keyword.operator.subpattern.lo",
                    "match": "@"
                },
                {
                    "comment": "dot access",
                    "name": "keyword.operator.access.dot.lo",
                    "match": "\\.(?!\\.)"
                },
                {
                    "comment": "ranges, range patterns",
                    "name": "keyword.operator.range.lo",
                    "match": "\\.{2}(=|\\.)?"
                },
                {
                    "comment": "colon",
                    "name": "keyword.operator.key-value.lo",
                    "match": ":(?!:)"
                },
                {
                    "comment": "dashrocket, skinny arrow",
                    "name": "keyword.operator.arrow.skinny.lo",
                    "match": "->"
                },
                {
                    "comment": "hashrocket, fat arrow",
                    "name": "keyword.operator.arrow.fat.lo",
                    "match": "=>"
                },
                {
                    "comment": "dollar macros",
                    "name": "keyword.operator.macro.dollar.lo",
                    "match": "\\$"
                },
                {
                    "comment": "question mark operator, questionably sized, macro kleene matcher",
                    "name": "keyword.operator.question.lo",
                    "match": "\\?"
                }
            ]
        },
        "interpolations": {
            "comment": "curly brace interpolations",
            "name": "meta.interpolation.lo",
            "match": "({)[^\"{}]*(})",
            "captures": {
                "1": {
                    "name": "punctuation.definition.interpolation.lo"
                },
                "2": {
                    "name": "punctuation.definition.interpolation.lo"
                }
            }
        },
        "lifetimes": {
            "patterns": [
                {
                    "comment": "named lifetime parameters",
                    "match": "(['])([a-zA-Z_][0-9a-zA-Z_]*)(?!['])\\b",
                    "captures": {
                        "1": {
                            "name": "punctuation.definition.lifetime.lo"
                        },
                        "2": {
                            "name": "entity.name.type.lifetime.lo"
                        }
                    }
                },
                {
                    "comment": "borrowing references to named lifetimes",
                    "match": "(\\&)(['])([a-zA-Z_][0-9a-zA-Z_]*)(?!['])\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.operator.borrow.lo"
                        },
                        "2": {
                            "name": "punctuation.definition.lifetime.lo"
                        },
                        "3": {
                            "name": "entity.name.type.lifetime.lo"
                        }
                    }
                }
            ]
        },
        "macros": {
            "patterns": [
                {
                    "comment": "macros",
                    "name": "meta.macro.lo",
                    "match": "(([a-z_][A-Za-z0-9_]*!)|([A-Z_][A-Za-z0-9_]*!))",
                    "captures": {
                        "2": {
                            "name": "entity.name.function.macro.lo"
                        },
                        "3": {
                            "name": "entity.name.type.macro.lo"
                        }
                    }
                }
            ]
        },
        "namespaces": {
            "patterns": [
                {
                    "comment": "namespace (non-type, non-function path segment)",
                    "match": "(?<![A-Za-z0-9_])([A-Za-z0-9_]+)((?<!super|self)::)",
                    "captures": {
                        "1": {
                            "name": "entity.name.namespace.lo"
                        },
                        "2": {
                            "name": "keyword.operator.namespace.lo"
                        }
                    }
                }
            ]
        },
        "types": {
            "patterns": [
                {
                    "comment": "numeric types",
                    "match": "(?<![A-Za-z])(f32|f64|i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)\\b",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.numeric.lo"
                        }
                    }
                },
                {
                    "comment": "parameterized types",
                    "begin": "\\b(_?[A-Z][A-Za-z0-9_]*)(<)",
                    "beginCaptures": {
                        "1": {
                            "name": "entity.name.type.lo"
                        },
                        "2": {
                            "name": "punctuation.brackets.angle.lo"
                        }
                    },
                    "end": ">",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.angle.lo"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#block-comments"
                        },
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                },
                {
                    "comment": "primitive types",
                    "name": "entity.name.type.primitive.lo",
                    "match": "\\b(void|bool|char|str)\\b"
                },
                {
                    "comment": "trait declarations",
                    "match": "\\b(trait)\\s+(_?[A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.declaration.trait.lo storage.type.lo"
                        },
                        "2": {
                            "name": "entity.name.type.trait.lo"
                        }
                    }
                },
                {
                    "comment": "struct declarations",
                    "match": "\\b(struct)\\s+(_?[A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.declaration.struct.lo storage.type.lo"
                        },
                        "2": {
                            "name": "entity.name.type.struct.lo"
                        }
                    }
                },
                {
                    "comment": "enum declarations",
                    "match": "\\b(enum)\\s+(_?[A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.declaration.enum.lo storage.type.lo"
                        },
                        "2": {
                            "name": "entity.name.type.enum.lo"
                        }
                    }
                },
                {
                    "comment": "type declarations",
                    "match": "\\b(type)\\s+(_?[A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.declaration.type.lo storage.type.lo"
                        },
                        "2": {
                            "name": "entity.name.type.declaration.lo"
                        }
                    }
                },
                {
                    "comment": "types",
                    "name": "entity.name.type.lo",
                    "match": "\\b_?[A-Z][A-Za-z0-9_]*\\b(?!!)"
                }
            ]
        },
        "gtypes": {
            "patterns": [
                {
                    "comment": "option types",
                    "name": "entity.name.type.option.lo",
                    "match": "\\b(Some|None)\\b"
                },
                {
                    "comment": "result types",
                    "name": "entity.name.type.result.lo",
                    "match": "\\b(Ok|Err)\\b"
                }
            ]
        },
        "punctuation": {
            "patterns": [
                {
                    "comment": "comma",
                    "name": "punctuation.comma.lo",
                    "match": ","
                },
                {
                    "comment": "curly braces",
                    "name": "punctuation.brackets.curly.lo",
                    "match": "[{}]"
                },
                {
                    "comment": "parentheses, round brackets",
                    "name": "punctuation.brackets.round.lo",
                    "match": "[()]"
                },
                {
                    "comment": "semicolon",
                    "name": "punctuation.semi.lo",
                    "match": ";"
                },
                {
                    "comment": "square brackets",
                    "name": "punctuation.brackets.square.lo",
                    "match": "[\\[\\]]"
                },
                {
                    "comment": "angle brackets",
                    "name": "punctuation.brackets.angle.lo",
                    "match": "(?<!=)[<>]"
                }
            ]
        },
        "strings": {
            "patterns": [
                {
                    "comment": "double-quoted strings and byte strings",
                    "name": "string.quoted.double.lo",
                    "begin": "(b?)(\")",
                    "beginCaptures": {
                        "1": {
                            "name": "string.quoted.byte.raw.lo"
                        },
                        "2": {
                            "name": "punctuation.definition.string.lo"
                        }
                    },
                    "end": "\"",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.definition.string.lo"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#escapes"
                        },
                        {
                            "include": "#interpolations"
                        }
                    ]
                },
                {
                    "comment": "double-quoted raw strings and raw byte strings",
                    "name": "string.quoted.double.lo",
                    "begin": "(b?r)(#*)(\")",
                    "beginCaptures": {
                        "1": {
                            "name": "string.quoted.byte.raw.lo"
                        },
                        "2": {
                            "name": "punctuation.definition.string.raw.lo"
                        },
                        "3": {
                            "name": "punctuation.definition.string.lo"
                        }
                    },
                    "end": "(\")(\\2)",
                    "endCaptures": {
                        "1": {
                            "name": "punctuation.definition.string.lo"
                        },
                        "2": {
                            "name": "punctuation.definition.string.raw.lo"
                        }
                    }
                },
                {
                    "comment": "characters and bytes",
                    "name": "string.quoted.single.char.lo",
                    "begin": "(b)?(')",
                    "beginCaptures": {
                        "1": {
                            "name": "string.quoted.byte.raw.lo"
                        },
                        "2": {
                            "name": "punctuation.definition.char.lo"
                        }
                    },
                    "end": "'",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.definition.char.lo"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#escapes"
                        }
                    ]
                }
            ]
        },
        "lvariables": {
            "patterns": [
                {
                    "comment": "self",
                    "name": "variable.language.self.lo",
                    "match": "\\b[Ss]elf\\b"
                },
                {
                    "comment": "super",
                    "name": "variable.language.super.lo",
                    "match": "\\bsuper\\b"
                }
            ]
        },
        "variables": {
            "patterns": [
                {
                    "comment": "variables",
                    "name": "variable.other.lo",
                    "match": "\\b(?<!(?<!\\.)\\.)(?:r#(?!(crate|[Ss]elf|super)))?[a-z0-9_]+\\b"
                }
            ]
        }
    }
}
